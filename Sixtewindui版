-- é«˜çº§æ£€æµ‹æŒ‰é’®
Tab:Button({
    Title = "é«˜çº§å®‰å…¨æ‰«æ",
    Desc = "æ‰§è¡Œæ·±åº¦å®‰å…¨æ£€æµ‹",
    Callback = function()
        WindUI:Notify({
            Title = "é«˜çº§æ‰«æ",
            Content = "æ­£åœ¨æ‰§è¡Œæ·±åº¦æ£€æµ‹...",
            Duration = 2
        })
        
        task.wait(1)
        
        local detectedFeatures = {}
        
        -- æ£€æµ‹å„ç§ç‰¹å¾
        if syn then table.insert(detectedFeatures, "âœ… Synapse X") end
        if krnl then table.insert(detectedFeatures, "âœ… Krnl") end
        if delta then table.insert(detectedFeatures, "âœ… Delta") end
        if sw then table.insert(detectedFeatures, "âœ… Script-Ware") end
        if fluxus then table.insert(detectedFeatures, "âœ… Fluxus") end
        if comet then table.insert(detectedFeatures, "âœ… Comet") end
        if electron then table.insert(detectedFeatures, "âœ… Electron") end
        if arceus then table.insert(detectedFeatures, "âœ… Arceus X") end
        
        -- æ£€æµ‹é«˜çº§ç‰¹å¾
        local advancedFeatures = {
            {"Hook Function", hookfunction},
            {"Clone Function", clonefunction},
            {"Get Calling Script", getcallingscript},
            {"Get Scripts", getscripts},
            {"Get GenV", getgenv}
        }
        
        for _, feature in ipairs(advancedFeatures) do
            local name, check = feature[1], feature[2]
            if check and type(check) == "function" then
                table.insert(detectedFeatures, "ğŸ”§ " .. name)
            end
        end
        
        if #detectedFeatures > 0 then
            local resultText = "æ£€æµ‹ç»“æœ:\n\n"
            for _, feature in ipairs(detectedFeatures) do
                resultText = resultText .. feature .. "\n"
            end
            resultText = resultText .. "\næ€»è®¡: " .. #detectedFeatures .. " ä¸ªç‰¹å¾"
            
            WindUI:Popup({
                Title = "é«˜çº§æ£€æµ‹ç»“æœ",
                Icon = "shield-alert",
                IconThemed = true,
                Content = resultText,
                Buttons = {
                    {
                        Title = "å¯¼å‡ºç»“æœ",
                        Icon = "download",
                        Callback = function()
                            setclipboard(resultText)
                            WindUI:Notify({
                                Title = "å·²å¤åˆ¶",
                                Content = "æ£€æµ‹ç»“æœå·²å¤åˆ¶åˆ°å‰ªè´´æ¿",
                                Duration = 3
                            })
                        end
                    },
                    {
                        Title = "å…³é—­",
                        Icon = "x",
                        Variant = "Secondary"
                    }
                }
            })
        else
            WindUI:Popup({
                Title = "æ£€æµ‹ç»“æœ",
                Icon = "shield-check",
                IconThemed = true,
                Content = "âœ… æœªæ£€æµ‹åˆ°ä»»ä½•æ³¨å…¥å™¨ç‰¹å¾\n\nç³»ç»ŸçŠ¶æ€: éå¸¸å®‰å…¨",
                Buttons = {
                    {Title = "ç¡®å®š", Icon = "check", Variant = "Primary"}
                }
            })
        end
    end
})

