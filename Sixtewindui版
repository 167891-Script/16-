-- æ³¨å…¥å™¨è‡ªåŠ¨æ£€æµ‹å‡½æ•°ï¼ˆåœ¨è„šæœ¬å¼€å§‹æ—¶è¿è¡Œï¼‰
local function autoDetectInjectors()
    task.wait(1) -- ç­‰å¾…WindUIåŠ è½½å®Œæˆ
    
    -- æ£€æµ‹å„ç§æ³¨å…¥å™¨
    local injectors = {
        {"Synapse X", syn, "rbxassetid://18660563116", Color3.fromHex("#FF5555")},
        {"Krnl", krnl, "rbxassetid://18660563117", Color3.fromHex("#5599FF")},
        {"Delta", delta, "rbxassetid://18660563122", Color3.fromHex("#AA55FF")},
        {"Script-Ware", sw, "rbxassetid://18660563118", Color3.fromHex("#FFAA55")},
        {"Fluxus", fluxus, "rbxassetid://18660563119", Color3.fromHex("#55FF55")},
        {"Comet", comet, "rbxassetid://18660563120", Color3.fromHex("#FF55FF")},
        {"Electron", electron, "rbxassetid://18660563124", Color3.fromHex("#FFFF55")},
        {"Arceus X", arceus, "rbxassetid://18660563126", Color3.fromHex("#FF5555")},
        {"Hydrogen", hydrogen, "rbxassetid://18660563127", Color3.fromHex("#55AAFF")},
        {"Oxygen U", oxygenu, "rbxassetid://18660563128", Color3.fromHex("#FF8855")}
    }
    
    local detected = {}
    
    for _, data in ipairs(injectors) do
        local name, check, image, color = unpack(data)
        if check then
            table.insert(detected, {name = name, image = image, color = color})
        end
    end
    
    if #detected > 0 then
        -- å»¶è¿Ÿæ˜¾ç¤ºé€šçŸ¥ï¼Œç¡®ä¿UIå®Œå…¨åŠ è½½
        task.wait(0.5)
        
        -- æ£€æµ‹åˆ°æ³¨å…¥å™¨
        local result = detected[1] -- å–ç¬¬ä¸€ä¸ªæ£€æµ‹åˆ°çš„
        
        WindUI:Notify({
            Title = "ğŸ” æ³¨å…¥å™¨è‡ªåŠ¨æ£€æµ‹",
            Content = "æ£€æµ‹åˆ°: " .. result.name,
            Duration = 5,
            Icon = result.image,
            Color = result.color
        })
        
        -- å¯é€‰ï¼šè‡ªåŠ¨å¼¹å‡ºè¯¦ç»†ç»“æœ
        task.wait(2)
        if #detected > 1 then
            local details = "è‡ªåŠ¨æ£€æµ‹åˆ° " .. #detected .. " ä¸ªæ³¨å…¥å™¨:\n\n"
            for i, inj in ipairs(detected) do
                details = details .. "âœ… " .. inj.name .. "\n"
            end
            details = details .. "\nä¸»æ³¨å…¥å™¨: " .. result.name
            
            WindUI:Popup({
                Title = "è‡ªåŠ¨æ£€æµ‹ç»“æœ",
                Icon = result.image,
                IconThemed = false,
                Content = details,
                Buttons = {
                    {
                        Title = "æŸ¥çœ‹è¯¦æƒ…",
                        Icon = "eye",
                        Callback = function()
                            local fullDetails = "å®Œæ•´æ£€æµ‹è¯¦æƒ…:\n\n"
                            for i, inj in ipairs(detected) do
                                fullDetails = fullDetails .. "â€¢ " .. inj.name .. "\n"
                            end
                            fullDetails = fullDetails .. "\næ³¨å…¥å™¨æ•°é‡: " .. #detected
                            
                            WindUI:Popup({
                                Title = "æ£€æµ‹è¯¦æƒ…",
                                Content = fullDetails,
                                Buttons = {
                                    {Title = "å…³é—­", Icon = "x"}
                                }
                            })
                        end
                    },
                    {
                        Title = "ç¡®å®š",
                        Icon = "check",
                        Variant = "Primary"
                    }
                }
            })
        end
        
        return detected
    else
        -- æœªæ£€æµ‹åˆ°æ³¨å…¥å™¨
        task.wait(0.5)
        WindUI:Notify({
            Title = "ğŸ”’ å®‰å…¨æ£€æµ‹",
            Content = "æœªæ£€æµ‹åˆ°å·²çŸ¥æ³¨å…¥å™¨",
            Duration = 4,
            Icon = "shield-check"
        })
        
        return {}
    end
end

-- åœ¨è„šæœ¬å¼€å§‹å¤„å¯åŠ¨è‡ªåŠ¨æ£€æµ‹
local autoDetectionThread = task.spawn(function()
    local detected = autoDetectInjectors()
    
    -- è®°å½•æ£€æµ‹ç»“æœï¼Œå¯åœ¨è®¾ç½®é¡µé¢æŸ¥çœ‹
    _G.InjectorDetectionResult = {
        time = os.date("%H:%M:%S"),
        detected = detected,
        count = #detected
    }
end)

