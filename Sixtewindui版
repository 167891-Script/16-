-- å¿«é€Ÿæ£€æµ‹æŒ‰é’®ï¼ˆä¾¿æ·ç‰ˆï¼‰
Tab:Button({
    Title = "å¿«é€Ÿæ£€æµ‹",
    Desc = "å¿«é€Ÿæ‰«æå¸¸è§æ³¨å…¥å™¨",
    Callback = function()
        local quickCheck = {
            {"Synapse X", syn},
            {"Krnl", krnl},
            {"Delta", delta},
            {"Script-Ware", sw}
        }
        
        local found = false
        local result = ""
        
        for _, data in ipairs(quickCheck) do
            local name, check = data[1], data[2]
            if check then
                found = true
                result = result .. "âœ… " .. name .. "\n"
            end
        end
        
        if found then
            WindUI:Popup({
                Title = "å¿«é€Ÿæ£€æµ‹ç»“æœ",
                Icon = "zap",
                IconThemed = true,
                Content = "æ£€æµ‹åˆ°ä»¥ä¸‹æ³¨å…¥å™¨:\n\n" .. result,
                Buttons = {
                    {Title = "ç¡®å®š", Icon = "check", Variant = "Primary"}
                }
            })
        else
            WindUI:Notify({
                Title = "å¿«é€Ÿæ£€æµ‹",
                Content = "æœªæ£€æµ‹åˆ°å¸¸è§æ³¨å…¥å™¨",
                Duration = 3,
                Icon = "check-circle"
            })
        end
    end
})

